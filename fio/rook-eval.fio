# Rook vol vs. EBS gp2 vs. EBS io1 6k iops
# 120 GiB ext4
# Block sizes: 4k, 16k, 128k, 1024k
# IO size 20 GiB

[global]
ioengine=libaio
iodepth=4
direct=1
runtime=60
invalidate=1
size=20g
filename=fio-test.file

###
#     EBS gp2 SSD, 120 GiB
#     block size 4k
#
[seq-read-4k-rook]
bs=4k
rw=read
stonewall
directory=/eval

[rand-read-4k-rook]
bs=4k
rw=randread
stonewall
directory=/eval

[seq-write-4k-rook]
bs=4k
rw=write
stonewall
directory=/eval

[rand-write-4k-rook]
bs=4k
rw=randwrite
stonewall
directory=/eval

